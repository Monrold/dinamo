			const productElement = link.closest(".product");

			if (productElement) {
				// Buscar el título solo dentro del contenedor del producto específico
				const productTitleElement = productElement.querySelector(
					".product-title",
				) as HTMLElement;

				if (productTitleElement) {
					const productTitle = productTitleElement.innerText;
					const message = `Me interesa este modelo: ${productTitle}`;
					const whatsappUrl = `https://wa.me/527711767689?text=${encodeURIComponent(message)}`;

					// Redirigir a la URL de WhatsApp con el mensaje
					window.location.href = whatsappUrl;
				}
			}
		});
	});
</script>
